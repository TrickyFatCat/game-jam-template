; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "BaseLevel",
"language": "GDScript",
"path": "res://src/Levels/BaseLevel.gd"
}, {
"base": "Position2D",
"class": "CameraController",
"language": "GDScript",
"path": "res://src/PlayerCamera/CameraController.gd"
}, {
"base": "KinematicBody2D",
"class": "Entity",
"language": "GDScript",
"path": "res://src/Entities/Entity.gd"
}, {
"base": "Entity",
"class": "Player",
"language": "GDScript",
"path": "res://src/Entities/Player/Player.gd"
} ]
_global_script_class_icons={
"BaseLevel": "",
"CameraController": "",
"Entity": "",
"Player": ""
}

[application]

config/name="game-jam-template"
config/icon="res://icon.png"

[autoload]

Utility="*res://singletons/Utility.gd"
Events="*res://singletons/Events.gd"
LevelController="*res://singletons/LevelController.gd"
